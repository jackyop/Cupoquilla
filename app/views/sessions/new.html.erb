      <%= f.text_field :email,class: "form-control"%>
  </div>
  <div class="input-group input-group-lg">
      <%= f.label :password,"Digite su contraseÃ±a",class: "input-group-addon" %>
      <%= f.password_field :password, class: "form-control" %>
  </div>
      <%= f.submit "Ingresar",class: "btn btn-lg btn-success button" %>
    <%end%>
</div>